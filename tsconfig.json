{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": [
            "es2015",
            "dom"
        ],
        "allowUnreachableCode": false,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "sourceMap": false,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true,
        "strictNullChecks": false,
        "preserveConstEnums": true
    }
}